[flake8]
max-line-length: 90
extend-ignore =
    # unexpected indentation (comment)
    E116
    # continuation line missing indentation or outdented
    E122
    # from import
    E125,E127,E128,E129,E201,E221,E225,E231,E261,E266,E301,E302,E303,E305,E501,E722,F403,F405
    # from application
    E125,E127,E128,E129,E201,E221,E225,E231,E261,E266,E301,E302,E303,E501,E722
    # from lib
    E222
